<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="box" alignment="CENTER" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane fx:id="matrixIn" alignment="CENTER" hgap="10.0" vgap="10.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField alignment="CENTER" />
            <TextField alignment="CENTER" GridPane.columnIndex="1" />
            <TextField alignment="CENTER" GridPane.columnIndex="2" />
            <TextField alignment="CENTER" GridPane.rowIndex="1" />
            <TextField alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField alignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <TextField alignment="CENTER" GridPane.rowIndex="2" />
            <TextField alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField alignment="CENTER" GridPane.columnIndex="2" GridPane.rowIndex="2" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </GridPane>
      <HBox fx:id="dimIn" alignment="CENTER" spacing="10.0">
         <children>
            <TextField fx:id="leftDimIn" alignment="CENTER" text="3" />
            <Label fx:id="by" text="x" />
            <TextField fx:id="rightDimIn" alignment="CENTER" text="3" />
         </children>
         <padding>
            <Insets bottom="8.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
      <Button fx:id="setBtn" mnemonicParsing="false" onAction="#resize" prefWidth="100.0" text="Set">
         <VBox.margin>
            <Insets bottom="8.0" />
         </VBox.margin>
      </Button>
   </children>
</fx:root>
